{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "extends": "airbnb-base/legacy",
  "rules": {
    "strict": 0,
    "no-use-before-define": [2, {"functions": false}],
    "max-len": [2, 120],
    "no-new": [0],
    "no-plusplus": [0],
    "no-underscore-dangle": [0],
    "no-mixed-operators": [0],
    "no-param-reassign": [0]
  }
}

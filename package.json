  "scripts": {
    "build": "NODE_ENV=production webpack --config front/webpack/production.js",
    "watch": "NODE_ENV=development webpack --config front/webpack/development.js",
    "test": "npm run lint:eslint && npm run lint:styles",
    "lint:js": "$(npm bin)/eslint --quiet front/ --ext .js",
    "lint:styles": "$(npm bin)/stylelint front/blocks/**/*.pcss",
    "precommit": "npm run precommit:js && npm run precommit:styles",
    "precommit:js": "git diff-index --cached HEAD --name-only --diff-filter ACMR | grep -E \"\\.js$\" | xargs eslint --quiet",
    "precommit:styles": "git diff-index --cached HEAD --name-only --diff-filter ACMR | grep -E \"\\.pcss$\" | xargs stylelint"
